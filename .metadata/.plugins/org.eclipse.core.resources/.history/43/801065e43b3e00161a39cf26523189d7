sentence = "the quick brown fox jumps over the lazy dog"
words = sentence.split()
word_lengths = []

for word in words:
    if word != "the":
        word_lengths.append(len(word));

print words
print word_lengths

def myfunction(first,second,third):
    print "First: %s" % first
    print "Second: %s" % second
    print "Third: %s" % third